{
  "name": "gen-srcset",
  "version": "0.0.1",
  "main": "src/index.js",
  "bin": {
    "gen-srcset": "./index.js"
  },
  "repository": "https://github.com/iakindev/gen-srcset",
  "author": "İlyas Akın <ashnwor@gmail.com>",
  "license": "MIT",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon --exec babel-node --extensions '.js,.ts' src/index.js",
    "build": "yarn run babel --extensions '.js,.ts' --ignore '**/__tests__' src -d dist --copy-files",
    "postbuild": "cp package.json dist && cp README.md dist",
    "pretest": "yarn run deliquus",
    "test": "jest"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.13",
    "commander": "^7.1.0",
    "sharp": "^0.27.1"
  },
  "devDependencies": {
    "@atolye15/eslint-config": "^1.2.0",
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.15",
    "@babel/preset-env": "^7.12.11",
    "@types/jest": "^26.0.20",
    "deliquus": "^0.3.7",
    "eslint": "^7.20.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "typescript": "^4.1.5"
  }
}
